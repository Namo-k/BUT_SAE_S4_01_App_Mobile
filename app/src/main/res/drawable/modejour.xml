<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">


    <item>
        <shape
            android:shape="oval">
            <solid
                android:color="@color/white"/>
            <size
                android:height="8dp"
                android:width="8dp"/>
            <stroke
                android:width="10dp"
                android:color="@color/darkBlueLight"/>

        </shape>
    </item>
    <item android:drawable="@drawable/sun"
        android:top="200dp"
        android:bottom="200dp"
        android:right="200dp"
        android:left="200dp"/>



</layer-list>